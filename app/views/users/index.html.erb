<div class="container">
  <div class="row">
    <h3>Users</h3>
    <div class="table-responsive">
      <table class="table table-striped table-bordered">
        <thead>
          <tr>
            <th>Name</th>
            <th>User Name</th>
            <th>Role</th>
            <th>State</th>
            <th colspan="2">Actions</th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>    
        </tbody>
      </table>
    </div>
  </div>
</div>

  
